<div class="countdown-title clearfix">
<div class="customtextcountdown">
<h4><span style="color: #e96a6a;">Special price Countdown:</span></h4>
</div>
<div id="countdown-footer"></div>
<div class="customtextcountdown">
<p>Custom text description module "Countdown Discount Timer Module"</p>
</div>
</div>
 
<script type="text/javascript">
$(function(){
	var ts = new Date(2015 ,1 , 28, 00, 00 , 00 );
	var newYear = true;
	if((new Date()) > ts){
		ts = (new Date()).getTime() + 10*24*60*60*1000;
		newYear = false;
	}
	$('#countdown-footer').countdown({
		timestamp	: ts,
		callback	: function(days, hours, minutes, seconds){
			var message = "";
			message += days + " jour" + ( days == 1 ? '':'s' ) + ", ";
			message += hours + " heure" + ( hours==1 ? '':'s' ) + ", ";
			message += minutes + " minute" + ( minutes==1 ? '':'s' ) + " et ";
			message += seconds + " seconde" + ( seconds==1 ? '':'s' );
		}
	});	
});
</script>



