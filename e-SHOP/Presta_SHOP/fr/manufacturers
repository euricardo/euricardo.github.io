<script>
	
		(function($){
			$(document).ready(function(){
				var $dur = "medium"; 
				$("#fbplikebox").css({left: -370 , top : 150 })
				$("#fbplikebox").hover(function () {
					$(this).stop().animate({
					left: 0
					}, $dur);
				}, function () {
					$(this).stop().animate({
					   left: -370
					}, $dur);
				});
				$("#fbplikebox").show();
			});
		})(jQuery);
			
	</script>
